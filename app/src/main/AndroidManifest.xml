<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hamishbrindle.bio_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Mapping.MappingActivity"></activity>
        <activity android:name=".Alerts.AlertsActivity"></activity>
        <activity android:name=".Analytics.AnalyticsActivity"></activity>
        <activity android:name=".Connect.ConnectActivity"></activity>

    </application>

</manifest>